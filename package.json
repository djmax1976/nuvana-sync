{
  "name": "nuvana",
  "version": "1.0.0",
  "description": "Local-first back-office data management for convenience stores",
  "main": "dist/main/index.js",
  "author": {
    "name": "Nuvana Inc.",
    "email": "support@nuvana.com"
  },
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://nuvana.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/nuvana/nuvana-sync.git"
  },
  "scripts": {
    "postinstall": "electron-rebuild",
    "dev": "node scripts/dev.js",
    "dev:app": "npm run build && release/win-unpacked/Nuvana.exe",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "package": "electron-builder --config electron-builder.config.js",
    "package:win": "electron-builder --win --config electron-builder.config.js",
    "build:win": "npm run build && electron-builder --win --config electron-builder.config.js",
    "build:win:unsigned": "npm run build && cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --win --config electron-builder.config.js -c.npmRebuild=false",
    "release": "npm run build && electron-builder --publish=always --config electron-builder.config.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:strict": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:security": "vitest run tests/security",
    "test:performance": "vitest run tests/performance",
    "test:integration": "vitest run --config vitest.integration.config.ts tests/integration",
    "test:regression": "vitest run tests/regression",
    "test:smoke": "vitest run tests/smoke",
    "test:sync": "vitest run tests/unit/services/*sync*.spec.ts tests/unit/dal/sync*.spec.ts tests/integration/*sync*.spec.ts tests/regression/*.spec.ts",
    "test:release-gate": "npm run test:smoke && npm run test:regression && npm run test:security",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:run && npm run test:integration && npm run test:e2e",
    "validate": "npm run typecheck && npm run lint && npm run format:check && npm run test:coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "axios": "^1.13.5",
    "bcrypt": "^6.0.0",
    "better-sqlite3-multiple-ciphers": "^12.5.0",
    "chokidar": "^4.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.3.0",
    "fast-xml-parser": "^5.3.6",
    "lucide-react": "^0.562.0",
    "node-machine-id": "^1.1.12",
    "react-hook-form": "^7.71.0",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.2",
    "@playwright/test": "^1.58.1",
    "@tanstack/react-query": "^5.90.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.19",
    "better-sqlite3": "^12.6.0",
    "cross-env": "^10.1.0",
    "electron": "^30.5.1",
    "electron-builder": "^24.13.0",
    "electron-vite": "^5.0.0",
    "env-cmd": "^11.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.38",
    "prettier": "^3.7.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.12.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vite": "^5.3.0",
    "vitest": "^4.0.16"
  },
  "build": {
    "extends": "./electron-builder.config.js"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
